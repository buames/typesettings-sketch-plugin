{
  "name": "typesettings-sketch-plugin",
  "version": "0.2.0",
  "description": "A quest for consistency",
  "engines": {
    "sketch": ">=3.0",
    "node": ">= 11.0.0"
  },
  "skpm": {
    "name": "Typesettings",
    "manifest": "src/plugin/manifest.json",
    "main": "typesettings.sketchplugin",
    "assets": [
      "src/assets/**/*",
      "src/directory/**/*"
    ],
    "resources": [
      "src/webview/**/*.js"
    ]
  },
  "scripts": {
    "clean": "rm -rf typesettings.sketchplugin",
    "build": "skpm-build",
    "watch": "skpm-build -w",
    "start": "skpm-build -w -r",
    "publish": "skpm publish",
    "postinstall": "yarn run build && skpm-link",
    "lint": "yarn lint:webview && yarn lint:plugin",
    "lint:webview": "eslint --ext .js,.jsx src/webview",
    "lint:plugin": "eslint --ext .js src/plugin"
  },
  "dependencies": {
    "@emotion/core": "^10.0.0",
    "@emotion/styled": "^10.0.0",
    "@skpm/dialog": "^0.2.3",
    "@skpm/fs": "^0.2.2",
    "@skpm/path": "^0.1.0",
    "deepmerge": "^2.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "sketch-module-web-view": "^2.1.1",
    "svg-inline-react": "^3.1.0",
    "typesettings-js": "^0.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@emotion/babel-plugin-core": "^0.6.9",
    "@skpm/builder": "^0.5.11",
    "@skpm/extract-loader": "^2.0.2",
    "babel-eslint": "10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-sketch": "^0.2.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "skpm": "1.1.10",
    "svg-inline-loader": "^0.8.0"
  },
  "lint-staged": {
    "*{.js,.jsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "homepage": "https://github.com/buames/typesettings-sketch-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buames/typesettings-sketch-plugin.git"
  },
  "author": "buames <jamesmakeseyes@gmail.com>",
  "license": "MIT"
}
